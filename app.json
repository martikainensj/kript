{
  "expo": {
    "name": "Kript",
    "slug": "kript",
    "scheme": "kript",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "ios": {
      "bundleIdentifier": "com.martikainensj.kript",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/MonaSans-Black.otf",
            "./assets/fonts/MonaSans-Bold.otf",
            "./assets/fonts/MonaSans-ExtraBold.otf",
            "./assets/fonts/MonaSans-ExtraLight.otf",
            "./assets/fonts/MonaSans-Light.otf",
            "./assets/fonts/MonaSans-Medium.otf",
            "./assets/fonts/MonaSans-Regular.otf",
            "./assets/fonts/MonaSans-SemiBold.otf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ef787ea0-6654-4af2-9fe7-6530d2d8a368"
      }
    },
    "android": {
      "package": "com.martikainensj.kript"
    }
  }
}
